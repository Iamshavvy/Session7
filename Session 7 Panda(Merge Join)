   "cell_type": "code",
   "execution_count": 195,
   "id": "acdc1112",
   "metadata": {},
   "outputs": [],
   "source": [
    "def create_gender(row):\n",
    "    if row[\"Age\"] < 19:\n",
    "        if row[\"Sex\"] == \"female\":\n",
    "            return \"girl\"\n",
    "        else:\n",
    "            return \"boy\"\n",
    "    else:\n",
    "        return row[\"Sex\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 198,
